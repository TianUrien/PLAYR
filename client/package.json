{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc -b && vite build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:coverage": "vitest run --coverage --reporter=verbose",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:smoke": "playwright test --grep @smoke --project=setup --project=chromium --project=chromium-player --project=chromium-club --project=chromium-brand --project=mobile-player",
    "test:e2e:auth": "playwright test --project=setup --project=chromium-player",
    "test:e2e:all": "playwright test",
    "test:all": "npm run lint && npm run typecheck && npm run test:unit && npm run build && npm run test:e2e:smoke",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:db": "vitest run --config vitest.db.config.ts",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@sentry/react": "^10.26.0",
    "@sentry/vite-plugin": "^4.6.1",
    "@supabase/supabase-js": "^2.81.1",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-virtual": "^3.13.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.545.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.64.0",
    "react-router-dom": "^7.9.4",
    "tslib": "^2.8.1",
    "tus-js-client": "^4.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.12",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "npm:rolldown-vite@7.1.14",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.12",
    "workbox-window": "^7.4.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}
